RED=$(tput setaf 1)
BLACK=$(tput setaf 0)
GREEN=$(tput setaf 2)
YELLOW=$(tput setaf 3)
LIME_YELLOW=$(tput setaf 190)
POWDER_BLUE=$(tput setaf 153)
BLUE=$(tput setaf 4)
MAGENTA=$(tput setaf 5)
CYAN=$(tput setaf 6)
WHITE=$(tput setaf 7)
BRIGHT=$(tput bold)
NORMAL=$(tput sgr0)
BLINK=$(tput blink)
REVERSE=$(tput smso)
UNDERLINE=$(tput smul)


echo "${RED}this is redRED ${NORMAL}this is normal"
echo "${BLACK}this is redBLACK ${NORMAL}this is normal"
echo "${GREEN}this is redGREEN ${NORMAL}this is normal"
echo "${YELLOW}this is redYELLOW ${NORMAL}this is normal"
echo "${LIME_YELLOW}this is redLIME_YELLOW ${NORMAL}this is normal"
echo "${POWDER_BLUE}this is redPOWDER_BLUE ${NORMAL}this is normal"
echo "${BLUE}this is redBLUE ${NORMAL}this is normal"
echo "${MAGENTA}this is redMAGENTA ${NORMAL}this is normal"
echo "${CYAN}this is redCYAN ${NORMAL}this is normal"
echo "${WHITE}this is redWHITE ${NORMAL}this is normal"
echo "${BRIGHT}this is redBRIGHT ${NORMAL}this is normal"
echo "${NORMAL}this is redNORMAL ${NORMAL}this is normal"
echo "${BLINK}this is redBLINK ${NORMAL}this is normal"
echo "${REVERSE}this is redREVERSE ${NORMAL}this is normal"
echo "${UNDERLINE}this is redUNDERLINE ${NORMAL}this is normal"


